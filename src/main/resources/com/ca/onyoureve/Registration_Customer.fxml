<GridPane prefHeight="400.0" prefWidth="491.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.ca.onyoureve.RegistrationCustomerController">
    <children>
        <AnchorPane prefHeight="400.0" prefWidth="601.0">


            <VBox fillWidth="false" layoutX="252.0" layoutY="19.0" spacing="20.0">

                <JFXTextField fx:id="cus_email" layoutX="125.0" layoutY="200.0" prefHeight="25.0" prefWidth="196.0" promptText="Email" VBox.vgrow="ALWAYS">


                </JFXTextField>


               <JFXTextField fx:id="cus_username" layoutX="125.0" layoutY="144.0" prefHeight="25.0" prefWidth="196.0" promptText="User Name" VBox.vgrow="ALWAYS">

               </JFXTextField>


               <JFXTextField fx:id="cus_fname" layoutX="125.0" layoutY="144.0" prefHeight="25.0" prefWidth="196.0" promptText="First Name" VBox.vgrow="ALWAYS">

                </JFXTextField>

                <JFXTextField fx:id="cus_lname" layoutX="125.0" layoutY="144.0" prefHeight="25.0" prefWidth="196.0" promptText="Last Name" VBox.vgrow="ALWAYS">

                </JFXTextField>


                <JFXTextField fx:id="cus_nid" layoutX="125.0" layoutY="144.0" prefHeight="25.0" prefWidth="196.0" promptText="NID" VBox.vgrow="ALWAYS">

                </JFXTextField>

                <JFXPasswordField fx:id="cus_password" layoutX="125.0" layoutY="144.0" prefHeight="25.0" prefWidth="196.0" promptText="Password" VBox.vgrow="ALWAYS">

                </JFXPasswordField>


                <DatePicker fx:id="cus_dob" layoutX="125.0" layoutY="144.0" prefHeight="25.0" prefWidth="196.0" promptText="Select Date Of Birth" VBox.vgrow="ALWAYS">

                </DatePicker>

                <Button fx:id="btn_reg" prefHeight="25.0" prefWidth="192.0" style="-fx-background-radius: 30;" text="Sign Up" />
                <padding>
                    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                </padding>

            </VBox>
         <ImageView fitHeight="241.0" fitWidth="233.0" layoutX="14.0" layoutY="79.0" pickOnBounds="true">
            <image>
               <Image url="@OnYourEVE-logos/OnYourEVE-logos_transparent.png" />
            </image>
         </ImageView>

        </AnchorPane>
    </children>
    <columnConstraints>
        <ColumnConstraints />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints />
    </rowConstraints>
</GridPane>
